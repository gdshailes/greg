<div class="QuickLinks">
	<div class="QuickLinksPanel">
		<h1>
      <div class="countdown">
        <span class="weeks">0</span> weeks
        <span class="days">0</span> days
        <span class="hours">0</span> hours
        <span class="minutes">0</span> minutes
        <span class="seconds">0</span> seconds
        until Rat Race start
      </div>
    </h1>
		<ul>
      <li><a href="/ratrace">Rat Race</a></li>
      <div class="LineBreak"> </div>
      <% if current_user %>
        <h1>Signed in as <%= current_user.first_name %></h1>
        <% if current_user.is_greg? %>
          <li><a href="/greghome">Greghome</a></li>
        <% elsif current_user.is_jo? %>
          <li><a href="/greghome/s382">Do not click me!</a></li>
        <% end %>
        <li><%= link_to 'Sign Out', destroy_user_session_path, method: 'DELETE' %></li>
      <% else %>
        <h1>Not signed in</h1>
        <li><a href="/users/sign_in">Sign In</a></li>
        <li><%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %></li>
        <li><%= link_to "Sign in with Google", user_google_omniauth_authorize_path %></li>
      <% end %>
		</ul>
	</div>
</div>
